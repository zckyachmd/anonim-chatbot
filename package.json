{
  "name": "anonim-chatbot",
  "version": "1.0.0",
  "description": "Anonymous Telegram Chatbot",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "pm2 start app.js --name anonim-chatbot",
    "stop": "pm2 stop anonim-chatbot",
    "restart": "pm2 restart anonim-chatbot",
    "dev": "pm2 start app.js --name anonim-chatbot --watch --time --log-date-format 'DD-MM HH:mm:ss.SSS' && pm2 monit"
  },
  "keywords": [
    "telegram",
    "chatbot",
    "anonymous"
  ],
  "author": "Zacky Achmad",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "cron": "^3.1.1",
    "telegraf": "^4.14.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "prisma": "^5.4.2"
  }
}
